{
    "version": 2,
    "name": "backend",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/src/index.js" }
    ],
    "env": {
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_HOST": "@db_host",
      "DB_DATABASE": "@db_database",
      "DB_PORT": "@db_port",
      "DB_SCHEMA": "@db_schema",
      "PORT": "@port"
    }
  }
  